{
  "name": "single_page_application",
  "version": "1.0.0",
  "description": "React mentoring",
  "main": "server/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development  node ./server",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore --fix",
    "prod": "cross-env NODE_ENV=production  node ./server",
    "test": "jest --coverage --watchAll --verbose",
    "upgradeSnaps": "jest --updateSnapshot",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "coveragePathIgnorePatterns": [
      "ErrorBoundary.jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/enzyme.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Artur Liup",
  "license": "MIT",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "@redux-offline/redux-offline": "^2.5.1",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.1.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "express": "latest",
    "history": "^4.10.1",
    "isomorphic-fetch": "^2.2.1",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-devtools": "^3.5.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^2.1.0",
    "styled-components": "^4.4.1",
    "webpack": "^4.41.2",
    "webpack-dev-middleware": "latest",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/theming": "^5.2.8",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.9.2",
    "css-loader": "^2.1.1",
    "enzyme-to-json": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-env-run": "^3.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-test-renderer": "latest",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
