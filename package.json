{
  "name": "single_page_application",
  "version": "1.0.0",
  "description": "React mentoring",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --hot --config webpack.dev.js",
    "build": "webpack -p --config webpack.prod.js",
    "test": "jest --coverage --watchAll --verbose",
    "upgradeSnaps": "jest --updateSnapshot"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "coveragePathIgnorePatterns": [
      "ErrorBoundary.jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/enzyme.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Artur Liup",
  "license": "MIT",
  "dependencies": {
    "@redux-offline/redux-offline": "^2.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "express": "latest",
    "node-fetch": "^2.6.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-devtools": "^3.5.0",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^2.1.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.9.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.0.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-env-run": "^3.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-test-renderer": "latest",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "redux-mock-store": "^1.5.3"
  }
}
